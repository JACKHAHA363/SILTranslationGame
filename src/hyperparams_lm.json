{
    "dataset": "wikitext103",
    "load_dataset": true,
    "save_dataset": false,

    "setup": "lm",
    "model": "RNN",

    "mode": "train",
    "gpu": 0,
    "seed": 19920722,

    "tie_emb": true,

    "D_emb": 512,
    "D_hid": 512,
    "n_layers": 1,
    "n_dir": 1,

    "eval_every": 1000,
    "save_at": [],
    "early_stop": false,
    "patience": -1,
    "batch_size": 512,
    "seq_len": 35,
    "drop_ratio": 0.2,

    "rep_pen_co": 0.0,

    "optimizer": "SGD",
    "grad_clip": 1.0,
    "anneal_by": 0.25,
    "plot_grad": false,
    "lr": 3e-4,
    "lr_anneal": "linear",
    "lr_min": 1e-5,
    "linear_anneal_steps": 25000,
    "max_training_steps": 50000,

    "debug": false
}
